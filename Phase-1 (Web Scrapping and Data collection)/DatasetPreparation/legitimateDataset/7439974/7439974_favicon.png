			<script type="application/javascript">
			    function saveClientNotes(event) {
			        
			        var oppo_hash = window.location.search;
			        var notes = document.getElementById("editors_notes").value;
			        
			        if(notes === "") {
			            swal("חובה להזין הערות לקוח","","error");
			            event.target.disabled = false; // enabling the submit button
			            return false;
			        }
			        
			        $.ajax({
                        type: "POST",
                        url: "/_ajax/ajax.index.php",
                        data: {
                            file: "opportunity",
                            action: "save_client_notes",
                            hash: oppo_hash,
                            notes: notes
                        },
                        dataType: "JSON",
                        success: function (response) {
                            if(response.error){
                                console.log("something went wrong",response.error);
                                swal("שגיאה: הערות לא נשמרו, נסו שוב.","","error");
                                event.target.disabled = false;
                            }
                            
                            if(response.answer){
                                swal({
                                    title: "",
					                text: "הערות נשמרו הצלחה",
                                    confirmButtonText: "אישור",
                                    closeOnClickOutside: false,
                                    showCancelButton: false,
                                    type: "success"
                                }, function(isConfirm) {
                                    if (isConfirm) {
                                        window.location.href = window.location.origin+window.location.pathname;
                                    }
                                },"success");
                            }
                        },
                        error : function (response) {  
                            console.error(response);
                            swal("שגיאה: הערות לא נשמרו, נסו שוב.","","error");
                            event.target.disabled = false;
                        },
                        // complete : function() {},
                    });
			    }
            </script>
