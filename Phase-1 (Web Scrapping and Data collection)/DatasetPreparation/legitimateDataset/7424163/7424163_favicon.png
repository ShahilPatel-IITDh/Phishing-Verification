<script>
    var updfDeviceSwiper

    function initUpdfDeviceSwiper() {
      let currentIndex = 0
      updfDeviceSwiper = new Swiper(".updf-device-swiper", {
        loop: true,
        effect: 'fade',
        fadeEffect: {
          crossFade: true,
        },
        pagination: {
          el: ".swiper-pagination",
          type: 'custom',
          renderCustom: function(swiper, current, total) {
            $('.swiper-pagination-item').removeClass('active').eq(current - 1).addClass('active')
            $('.swiper-pagination').removeClass(`current-index-${currentIndex}`).addClass(`current-index-${current - 1}`)
            currentIndex = current - 1
          }
        },
        autoplay: {
          delay: 5000,
          disableOnInteraction: false
        }
      })
      updfDeviceSwiper.el.onmouseover = function() {
        updfDeviceSwiper.autoplay.stop();
      }
      updfDeviceSwiper.el.onmouseout = function() {
        updfDeviceSwiper.autoplay.start();
      }
      $('.swiper-pagination .swiper-pagination-item').click(function() {
        updfDeviceSwiper.slideTo($(this).index() + 1)
      })
      $('.signature-video')[0].play()
      $('.signature-video')[0].playbackRate = 2.5
    }
  </script>
